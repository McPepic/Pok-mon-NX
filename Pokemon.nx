SUB LOADCOLL
  RESTORE COLLISION
	FOR I = 0 TO 255
		READ TILES
		FOR Y = 1 TO 0 STEP -1
			FOR X = 1 TO 0 STEP -1
				COLLISION(I, X, Y) = TILES AND $F
				TILES = TILES \ 16
			NEXT X
		NEXT Y
	NEXT I
END SUB
