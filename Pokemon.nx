SUB LOADTYPE
	RESTORE TYPE_DATA
	FOR I = 0 TO UBOUND(TYPE$)
		READ TYPE$(I)
	NEXT I
END SUB
TYPE_DATA:
DATA "NORMAL","FIRE"

SUB LOADMOVE
	RESTORE MOVE_DATA
	FOR I = 1 TO UBOUND(MOVENAME$)
		READ MOVENAME$(I),MOVETYPE(I),MOVECAT$(I),MOVEPOW(I),MOVEACC(I),MOVEPP(I)
	NEXT I
END SUB
MOVE_DATA:
DATA "TACKLE",0,"PHYSICAL",35,95,35

SUB LOADPOKE
	RESTORE POKEMON_DATA
	FOR I = 1 TO UBOUND(POKENAME$)
		READ POKENAME$(I),POKETYPE(I,0),POKETYPE(I,1)
		READ POKEHP(I),POKEATK(I),POKEDEF(I),POKESPATK(I),POKESPDEF(I),POKESPEED(I)
		FOR J = 0 TO UBOUND(POKEMOVEID, 2)
			READ POKEMOVEID(I,J), POKEMOVELV(I,J)
			IF POKEMOVEID(I,J) = 0 THEN EXIT
		NEXT J
	NEXT I
END SUB
