REM *** POKEMON GEN 2 RECREATION ***
REM ********** BY MCPEPIC **********
REM
REM CREDITS:
REM   GAME FREAK - ORIGINAL GAME
REM 	MATDEMO159 - PLAYER SPRITE

'GLOBAL VARIABLES
GLOBAL PLX, PLY, PLTX, PLTY, PLDIR, MOVE, CAMX, CAMY, CMAPX, CMAPY, INGRASS, TEXTSPEED, OUT
'CHUNK ID, X OFFSET, Y OFFSET
DIM GLOBAL COLLISION(255, 1, 1)
DIM GLOBAL MAP(7, 7)

TYPENUM = 1
MOVENUM = 1
POKENUM = 1
TEAMNUM = 5

'NAME
DIM GLOBAL TYPE$(TYPENUM)

'NAME,TYPE,CATEGORY,POWER,ACCURACY
DIM GLOBAL MOVENAME$(MOVENUM),MOVETYPE(MOVENUM),MOVECAT$(MOVENUM),MOVEPOW(MOVENUM),MOVEACC(MOVENUM),MOVEPP(MOVENUM)

'NAME,TYPE,MOVEID,MOVELV,
'HEALTH,ATTACK,DEFENSE,SPECIAL ATTACK,SPECIAL DEFENSE,SPEED
DIM GLOBAL POKENAME$(POKENUM),POKETYPE(POKENUM,1),POKEMOVEID(POKENUM,9),POKEMOVELV(POKENUM,9)
DIM GLOBAL POKEHP(POKENUM),POKEATK(POKENUM),POKEDEF(POKENUM),POKESPATK(POKENUM),POKESPDEF(POKENUM),POKESPEED(POKENUM)

'ID,NAME,XP,LEVEL,CURRENT HEALTH,STATUS EFFECT,MOVES,PP
'HP IV,ATTACK IV,DEFENSE IV,SPECIAL ATTACK IV,SPECIAL DEFENSE IV,SPEED IV,
'HP EV,ATTACK EV,DEFENSE EV,SPECIAL ATTACK EV,SPECIAL DEFENSE EV,SPEED EV
DIM GLOBAL TEAMID(TEAMNUM),TEAMNAME$(TEAMNUM),TEAMXP(TEAMNUM),TEAMLV(TEAMNUM),TEAMHP(TEAMNUM),TEAMSTATUS$(TEAMNUM),TEAMMOVE(TEAMNUM,3),TEAMPP(TEAMNUM,3)
DIM GLOBAL TEAMHPIV(TEAMNUM),TEAMATIV(TEAMNUM),TEAMDEIV(TEAMNUM),TEAMSAIV(TEAMNUM),TEAMSDIV(TEAMNUM),TEAMSPIV(TEAMNUM)
DIM GLOBAL TEAMHPEV(TEAMNUM),TEAMATEV(TEAMNUM),TEAMDEEV(TEAMNUM),TEAMSAEV(TEAMNUM),TEAMSDEV(TEAMNUM),TEAMSPEV(TEAMNUM)

'SETUP
PLX = 16 * 4
PLY = 16 * 4
PLTX = PLX
PLTY = PLY

CALL LOADCOLL
CMAPX = -1
CMAPY = -1

TEXTSPEED = 2

CALL LOADTYPE
CALL LOADMOVE
CALL LOADPOKE

DIM M$(3)


'===ASSIGN FIRST POKEMON===
TEAMID(0) = 1
TEAMNAME$(0) = "SPOT"
TEAMLV(0) = 5
'SET TO MAX AFTER CALCULATED
TEAMHP(0) = 0
TEAMSTATUS$(0) = ""

TEAMMOVE(0,0) = 1
TEAMPP(0,0) = MOVEPP(TEAMMOVE(0,0))

TEAMMOVE(0,1) = 1
TEAMPP(0,1) = MOVEPP(TEAMMOVE(0,1))

TEAMHPIV(0) = RND
TEAMATIV(0) = RND
TEAMDEIV(0) = RND
TEAMSAIV(0) = RND
TEAMSDIV(0) = RND
TEAMSPIV(0) = RND
TEAMHPEV(0) = 0
TEAMATEV(0) = 0
TEAMDEEV(0) = 0
TEAMSAEV(0) = 0
TEAMSDEV(0) = 0
TEAMSPEV(0) = 0
CALL CALCSTAT(POKEHP(TEAMID(0)), TEAMLV(0), TEAMHPIV(0), TEAMHPEV(0), TEAMHP(0))
'==========================


RANDOMIZE TIMER
GAMEPAD 1

'MAIN LOOP
MAIN:
CLS
SPRITE OFF

COPY ROM(1), 32 TO $FF00
FILL $8000 + 16 * 192, 16, $FF

DO
  'GET INPUT
  H = LEFT(0) - RIGHT(0)
  V =   UP(0) -  DOWN(0)
  
	'PLAYER UPDATE
  GOSUB PLMOVE
  GOSUB PLANIM
	
	'MOVE CAMERA
	CAMX = PLX - 64
	CAMY = PLY - 64
	
	CALL DRAWBG(CAMX, CAMY)
	
	'MOVE BACKGROUND
	SCROLL 1, CAMX AND 7, CAMY AND 7
	SCROLL 0, CAMX AND 7, CAMY AND 7
  
  'COPY PLAYER SPRITE TO CHARACTER MEMORY
  CALL ROM2CHAR(5, PLFRM, 64, 2, 2)
  
	PLSX = PLX - CAMX
	PLSY = PLY - CAMY - 4
	
  'DISPLAY PLAYER
  SPRITE 63, PLSX, PLSY, 64
  SPRITE 63 SIZE 1 FLIP PLFLX, 0
	
	IF INGRASS THEN
		SPRITE 62, PLSX	- 1, PLSY + 12, 6
		SPRITE 61, PLSX + 8, PLSY + 12, 6
		SPRITE 62 PAL 1
		SPRITE 61 PAL 1 FLIP 1,0
	ELSE
		SPRITE OFF 61 TO 62
	END IF
  
  'RUN ONCE A FRAME
  WAIT VBL
LOOP
PLMOVE:
	'IF PLAYER IS AT THE TARGET TILE
	IF PLX = PLTX AND PLY = PLTY THEN
		'INTERACTION
		IF BUTTON TAP(0,1) THEN
			'GET FACING TILE
			FACEX = PLX
			FACEY = PLY
			IF PLDIR = 0 THEN ADD FACEY, 16 ELSE IF PLDIR = 1 THEN ADD FACEY, -16 ELSE IF PLDIR = 2 THEN ADD FACEX, -16 ELSE ADD FACEX, 16
			
			CALL GETTILE(FACEX, FACEY)
			'SIGN
			IF OUT = 2 THEN
				CALL TEXTBOX("...")
			END IF
		END IF
		
		'IF THE PLAYER IS MOVING
		IF H <> 0 OR V <> 0 THEN
			'SET NEW TARGET IN HORIZONTAL OR VERTICAL DIRECTION
			IF H THEN
			ADD PLTX, H * 16
			IF H < 0 THEN PLDIR = 2 ELSE PLDIR = 3
			ELSE
			ADD PLTY, V * 16
			IF V < 0 THEN PLDIR = 1 ELSE PLDIR = 0
			END IF
			MOVE = -1
			
			'RUNNING SHOES (KEEP SPEED AT [1, 2, 4, 8, OR 16] TO AVOID ISSUES)
			IF BUTTON(0,0) THEN PLSPEED = 2 ELSE PLSPEED = 1
			
			CALL GETTILE(PLTX, PLTY)
			'WALL
			IF OUT = 1 OR OUT = 2 OR OUT = 9 OR ((OUT >= 5 AND OUT <= 8) AND OUT <> PLDIR + 5) THEN
				PLTX = PLX
				PLTY = PLY
			END IF
		ELSE
			MOVE = 0
		END IF
	ELSE
		'SLIDE THE PLAYER TOWARDS THE TARGET POSITION
		ADD PLX, SGN(PLTX - PLX) * PLSPEED
		ADD PLY, SGN(PLTY - PLY) * PLSPEED
		
		'PLAYER ENTERED A NEW TILE
		IF PLX = PLTX AND PLY = PLTY THEN
			CALL GETTILE(PLX, PLY)
			
			IF OUT = 3 THEN CALL WARP(RND(15),RND(15))
			
			INGRASS = OUT = 4
			IF INGRASS AND RND < 0.1 THEN
				GOTO BATTLE
			END IF
		END IF
	END IF
RETURN
PLANIM:
  'PLAYER ANIMATION
  PLFRM = 2 * PLDIR
  PLFLX = 0
  IF PLDIR = 3 THEN
    ADD PLFRM, -2
		PLFLX = -1
  END IF
  IF MOVE THEN
    'ANIMATION TIME
    WTIME = TIMER * 0.1
		WMOD = WTIME AND 1
		WDIV = (WTIME \ 2) AND 1
	
    IF PLDIR = 0 THEN
			ADD PLFRM, 32 * WMOD
      PLFLX = WDIV
    ELSE IF PLDIR = 1 THEN
			ADD PLFRM, 32 * WMOD
      PLFLX = WDIV
    ELSE IF PLDIR = 2 THEN
      ADD PLFRM, 32 * WMOD
    ELSE
			ADD PLFRM, 32 * WMOD
    END IF
  END IF
RETURN

BATTLE:
'TRACE "BATTLE"
'DISPLAY TRANSITION (SUB)
CALL TRANSITION

COPY ROM(6), 32 TO $FF00
FILL $8000 + 16 * 192, 16

CLS

ENEMYID = 0
ENEMYLV = 0
ENEMYHP = 0
ENEMYAT = 0
ENEMYDE = 0
ENEMYSA = 0
ENEMYSD = 0
ENEMYSP = 0

PLAYERID = 0
PLAYERLV = 0
PLAYERHP = 0
PLAYERAT = 0
PLAYERDE = 0
PLAYERSA = 0
PLAYERSD = 0
PLAYERSP = 0

'ENCOUNTER TABLE
CHX = INT(PLX / 256)
CHY = INT(PLY / 256)
'TRACE STR$(CHX)+","+STR$(CHY)
IF CHX = -1 AND CHY = 0 THEN
	RESTORE ENCOUNTER_0
ELSE
	RESTORE ENCOUNTER_0
END IF
RANDOM = RND
CHANCE = 0
NEXT_ENCOUNTER:
READ ID, LV, CH
ADD CHANCE, CH
IF RANDOM < CHANCE THEN
	ENEMYID = ID
	ENEMYLV = LV
ELSE
	GOTO NEXT_ENCOUNTER
END IF

'GENERATE IVS FOR ENEMY POKEMON
HPIV = RND
ATIV = RND
DEIV = RND
SAIV = RND
SDIV = RND
SPIV = RND

'GET ENEMY STATS
CALL CALCSTAT(   POKEHP(ENEMYID),ENEMYLV,HPIV,0,ENEMYHP)
CALL CALCSTAT(  POKEATK(ENEMYID),ENEMYLV,ATIV,0,ENEMYAT)
CALL CALCSTAT(  POKEDEF(ENEMYID),ENEMYLV,DEIV,0,ENEMYDE)
CALL CALCSTAT(POKESPATK(ENEMYID),ENEMYLV,SAIV,0,ENEMYSA)
CALL CALCSTAT(POKESPDEF(ENEMYID),ENEMYLV,SDIV,0,ENEMYSD)
CALL CALCSTAT(POKESPEED(ENEMYID),ENEMYLV,SPIV,0,ENEMYSP)

TRACE ENEMYHP
TRACE ENEMYAT
TRACE ENEMYDE
TRACE ENEMYSA
TRACE ENEMYSD
TRACE ENEMYSP
TRACE ""

ENEMYCHP = ENEMYHP

'GET PLAYER STATS
PLAYERID = TEAMID(0)
PLAYERLV = TEAMLV(0)

CALL CALCSTAT(   POKEHP(TEAMID(0)),TEAMLV(0),TEAMHPIV(0),TEAMHPEV(0),PLAYERHP)
CALL CALCSTAT(  POKEATK(TEAMID(0)),TEAMLV(0),TEAMATIV(0),TEAMATEV(0),PLAYERAT)
CALL CALCSTAT(  POKEDEF(TEAMID(0)),TEAMLV(0),TEAMDEIV(0),TEAMDEEV(0),PLAYERDE)
CALL CALCSTAT(POKESPATK(TEAMID(0)),TEAMLV(0),TEAMSAIV(0),TEAMSAEV(0),PLAYERSA)
CALL CALCSTAT(POKESPDEF(TEAMID(0)),TEAMLV(0),TEAMSDIV(0),TEAMSDEV(0),PLAYERSD)
CALL CALCSTAT(POKESPEED(TEAMID(0)),TEAMLV(0),TEAMSPIV(0),TEAMSPEV(0),PLAYERSP)

TRACE PLAYERHP
TRACE PLAYERAT
TRACE PLAYERDE
TRACE PLAYERSA
TRACE PLAYERSD
TRACE PLAYERSP
TRACE ""
TRACE ""

PLAYERCHP = TEAMHP(0)

RESET_BATTLE_UI:
CLS

'DISPLAY ENEMY INFO
TEXT 1, 1, POKENAME$(ENEMYID)
TEXT 1, 2, "LV: " + STR$( ENEMYLV )

'DISPLAY PLAYER INFO
TEXT 9, 8, POKENAME$(TEAMID(0))
TEXT 9, 9, "LV: " + STR$( TEAMLV(0) )

'PLAYER & ENEMY SPRITE
SPRITE 0, 8, 128 - 64, 128
SPRITE 1, 160 - 40, 8, 132
SPRITE 0 SIZE 3
SPRITE 1 SIZE 3

'DISPLAY UI BOX
CALL UIBOX(0,12,19,15)

'SETUP MENU
SELX = 7
SELY = 13
TEXT 8,13,"ATTACK PKMN"
TEXT 8,14,"ITEM   RUN "

DO
	'DISPLAY CURSOR
	CELL SELX,SELY,192
	IF UP TAP(0) OR DOWN TAP(0) THEN ADD SELY, 1, 13 TO 14
	IF LEFT TAP(0) OR RIGHT TAP(0) THEN ADD SELX, 7, 7 TO 14
	CELL SELX,SELY,222
	
	'DISPLAY ENEMY HP
	HPBARW = 5
	BARFILL = (ENEMYCHP / ENEMYHP) * (HPBARW * 8)
	FOR I = 1 TO HPBARW
		HPTILE = MAX(0, MIN(BARFILL, 8))
		IF HPTILE THEN CELL I + 1, 3, 135 + INT(HPTILE) ELSE CELL I + 1, 3, 0
		ADD BARFILL, -8
	NEXT I
	
	'RECOGNIZE THE OPTION
	IF BUTTON TAP(0,0) THEN
		IF SELX = 7 THEN
			IF SELY = 13 THEN
				GOSUB ATTACK
			ELSE
				GOSUB ITEM
			END IF
		ELSE
			IF SELY = 13 THEN
				GOSUB PKMN
			ELSE
				GOSUB RUN
			END IF
		END IF
	END IF
	
	WAIT VBL
LOOP
ATTACK:
	'TRACE "ATTACK"
	WAIT VBL
	
	NUMMOVES = 0
	FOR I = 0 TO 3
		IF TEAMMOVE(0,I) THEN INC NUMMOVES ELSE EXIT
	NEXT I
	FOR I = 0 TO UBOUND(M$)
		M$(I) = MOVENAME$(TEAMMOVE(0,I))
	NEXT I
	
	INDEX = 0
	DO
		'DISPLAY CURSOR
		IF   UP TAP(0) THEN ADD INDEX, -1, 0 TO NUMMOVES - 1
		IF DOWN TAP(0) THEN ADD INDEX,  1, 0 TO NUMMOVES - 1
		CALL DROPDOWN(9, 9 + (4 - NUMMOVES), 17, 14, M$(), INDEX)
		
		'TRACE NUMMOVES
		
		'ACCEPT SELECTION
		IF BUTTON TAP(0,0) THEN 
			'TRACE INDEX
			'TRACE ""
			'TRACE ""
			
			DAMAGE = MOVEPOW(TEAMMOVE(0,INDEX))
			'TRACE DAMAGE
			'TRACE ""
			
			ENEMYCHP = MAX(0, MIN(ENEMYCHP - DAMAGE, ENEMYHP))
			'TRACE STR$(ENEMYCHP)+"/"+STR$(ENEMYHP)
			
			IF ENEMYCHP = 0 THEN GOTO PLAYERWIN
			
			EXIT
		END IF
		
		'RETURN TO MAIN SCREEN
		IF BUTTON TAP(0,1) THEN EXIT
		
		WAIT VBL
	LOOP
	WAIT VBL
RETURN RESET_BATTLE_UI
ITEM:
	TRACE "ITEM"
RETURN
PKMN:
	TRACE "PKMN"
RETURN
RUN:
	TRACE "RUN"
	GOTO MAIN
RETURN

PLAYERWIN:
	'GET XP
	ADD TEAMXP(0), ENEMYLV / PLAYERLV * 100
	
	'GET EV
	HS = MAX(ENEMYHP, MAX(ENEMYAT, MAX(ENEMYDE, MAX(ENEMYSA, MAX(ENEMYSD, ENEMYSP)))))
	IF HS = ENEMYHP THEN INC TEAMHPEV(0) ELSE IF HS = ENEMYAT THEN INC TEAMATEV(0) ELSE IF HS = ENEMYDE THEN INC TEAMDEEV(0) ELSE IF HS = ENEMYSA THEN INC TEAMSAEV(0) ELSE IF HS = ENEMYSD THEN INC TEAMSDEV(0) ELSE INC TEAMSPEV(0)
	
GOTO MAIN

SUB TRANSITION
	PAL 2
	PRIO 1
	FOR I = 0 TO 1000
		TEXT RND(20), RND(16), " "
		IF RND < 0.08 THEN WAIT VBL
	NEXT I
	
	CLS
	SPRITE OFF
	
	PRIO 0
	BG FILL 0, 0 TO 31, 31 CHAR 192
END SUB

SUB CALCSTAT(BASE,LV,IV,EV,OUT)
	OUT = INT(BASE + LV * (EV * 0.01 + IV + 1))
END SUB

'INPUT: 
'ROM INDEX
'SOURCE INDEX 
'DESTINATION INDEX
'CHARACTER WIDTH
'CHARACTER HEIGHT
'OUTPUT:
'MOVES A WIDTH X HEIGHT AREA OF CHARACTERS IN ROM STARTING WITH THE SOURCE CHARACTER TO THE DESTINATION CHARACTER
SUB ROM2CHAR(R,S,D,W,H)
	SRC = ROM(R) + 16 * S
	DST = $8000  + 16 * D
	FOR Y = 0 TO H - 1
	  COPY SRC, 16 * W TO DST
		
		ADD SRC, 256
		ADD DST, 256
	NEXT Y
END SUB

SUB UIBOX(X1,Y1,X2,Y2)
	PAL 2
	
	BG 1
	BG FILL X1, Y1 TO X2, Y2 CHAR 192
	
	BG 0
	BG FILL X1, Y1 TO X2, Y2 CHAR 192
	
	PAL 0
	'251 252 251
	'253     253
	'254 252 254
	FLIP 0,0
	BG FILL X1, Y1 TO X2, Y1 CHAR 252
	FLIP 0,1
	BG FILL X1, Y2 TO X2, Y2 CHAR 252
	FLIP 0,0
	BG FILL X1, Y1 TO X1, Y2 CHAR 253
	FLIP 1,0
	BG FILL X2, Y1 TO X2, Y2 CHAR 253
	
	FLIP 0,0
	CELL X1, Y1, 251
	CELL X1, Y2, 254
	FLIP 1,0
	CELL X2, Y1, 251
	CELL X2, Y2, 254
	FLIP 0,0
END SUB
SUB TEXTBOX(T$)
	CALL UIBOX(0,12,19,15)
	BG 0
	
	TEXTX = 1
	TEXTY = 13
	DELAY = -1
	
	I = 1
	REPEAT
		'NEXT WORD WON'T FIT ON CURRENT LINE
		IF (INSTR(T$," ",I) - I + TEXTX) > 19 THEN
			'GO TO NEXT LINE
			TEXTX = 1
			INC TEXTY
			
			'TEXTBOX FULL
			IF TEXTY = 15 THEN
				'DISPLAY ARROW
				FLIP 0,0
				TEXT 9,15,"_"
				FLIP 1,0
				TEXT 10,15,"_"
				FLIP 0,0
				
				'WAIT UNTIL BUTTON IS PRESSED
				REPEAT
				UNTIL BUTTON TAP(0)
				
				'RESET TEXTBOX
				TEXTY = 13
				CALL UIBOX(0,12,19,15)
				
				DELAY = -1
			END IF
		END IF
		
		'DISPLAY THE NEXT CHARACTER
		CC$ = MID$(T$,I,1)
		IF CC$ <> " " THEN TEXT TEXTX,TEXTY,CC$
		INC TEXTX
		
		'SELECT NEXT CHARACTER AND ADD A DELAY UNLESS IT IS DISABLED BY PRESSING THE SPEED-UP BUTTON
		INC I
		IF DELAY THEN
			FOR W = 1 TO (10 - TEXTSPEED)
				WAIT VBL
				IF BUTTON TAP(0) THEN DELAY = 0
			NEXT W
		END IF
	UNTIL I = LEN(T$) + 1
	
	'WAIT UNTIL BUTTON IS PRESSED
	REPEAT
	UNTIL BUTTON TAP(0)
	BG 1
	CLS 0
END SUB
SUB DROPDOWN(X1,Y1,X2,Y2,A$(),LI)
	'TRACE LI
	CALL UIBOX(X1,Y1,X2,Y2)
	
	CURX = X1 + 1
	CURY = Y1 + 1 + LI
	TEXT CURX, MIN(CURY, Y2 - 1), ">"
	
	IH = Y2 - Y1 - 2
	II = MAX(0, LI - IH)
	'TRACE II
	FOR I = 0 TO IH
		TEXT X1 + 2, Y1 + 1 + I, A$(II)
		INC II
	NEXT I
END SUB

SUB WARP(TILEX,TILEY)
	PLX = TILEX * 16
	PLY = TILEY * 16
	PLTX = PLX
	PLTY = PLY
END SUB

SUB GETTILE(PX,PY)
	'LOAD CURRENT BACKGROUND
	CALL LOADBG(INT(PX / 256), INT(PY / 256))
		
	PLCX = (PX \ 16) AND 15
	PLCY = (PY \ 16) AND 15
	OUT = COLLISION(MAP(PLCX \ 2, PLCY \ 2), PLCX MOD 2, PLCY MOD 2)
END SUB

'TAKE COORDINATE, DRAW VISIBLE AREA TO SCREEN
SUB DRAWBG(X,Y)
  'STORE CURRENT CELL COORDINATES
	STARTX = -(INT(X / 8) AND 31)
	STARTY = -(INT(Y / 8) AND 31)
	
	CELLX = STARTX
	CELLY = STARTY
	
	BG 1
  'FOR EACH MAP ON SCREEN
	FOR MAPY = INT(Y / 256) TO INT((Y + 127) / 256)
	  FOR MAPX = INT(X / 256) TO INT((X + 159) / 256)
			CALL LOADBG(MAPX, MAPY)
			
			CALL DRAWMAP(CELLX,CELLY)
			
			ADD CELLX, 32
		NEXT MAPX
		CELLX = STARTX
		ADD CELLY, 32
	NEXT MAPY
END SUB
SUB DRAWMAP(CX,CY)
	MINX = MAX(0, INT(-CX / 4))
	MAXX = MIN(INT((-CX + 20) / 4), 7)
	MINY = MAX(0, INT(-CY / 4))
	MAXY = MIN(INT((-CY + 16) / 4), 7)
	
	FOR Y = MINY TO MAXY
		FOR X = MINX TO MAXX
		  XX = CX + X * 4
			YY = CY + Y * 4
			
			SRC = MAP(X, Y)
			SRCX = (SRC MOD 16) * 4
			SRCY = (SRC  \  16) * 4
			
			BG COPY SRCX, SRCY, 4, 4 TO XX, YY
		NEXT X
	NEXT Y
END SUB

SUB LOADBG(MX,MY)
	IF MX = CMAPX AND MY = CMAPY THEN EXIT SUB
	CMAPX = MX
	CMAPY = MY
	
	IF MX = 0 AND MY = 0 THEN
		RESTORE NEW_BARK
	ELSE IF MX = 0 AND MY = -1 THEN
		RESTORE NEW_BARK_TOP
	ELSE IF MX = -1 AND MY = 0 THEN
		RESTORE ROUTE29_0
	ELSE IF MX = -2 AND MY = 0 THEN
		RESTORE ROUTE29_1
	ELSE IF (MX = -1 AND MY = -1) OR ((MX = -2 OR MX = -1) AND MY = 1) OR (MX = 0 AND MY = 1) THEN
		RESTORE TREES_BG
	ELSE
		RESTORE EMPTY_BG
	END IF
	
  FOR Y = 0 TO 7
	  FOR X = 0 TO 7
		  READ MAP(X, Y)
		NEXT X
	NEXT Y
END SUB
