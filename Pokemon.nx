REM *** POKEMON GEN 2 RECREATION ***
REM ********** BY MCPEPIC **********
REM
REM CREDITS:
REM   GAME FREAK - ORIGINAL GAME
REM 	MATDEMO159 - PLAYER SPRITE

'GLOBAL VARIABLES
GLOBAL PLX, PLY, PLTX, PLTY, PLDIR, MOVE, CAMX, CAMY, CMAPX, CMAPY, INGRASS, TEXTSPEED, OUT
'CHUNK ID, X OFFSET, Y OFFSET
DIM GLOBAL COLLISION(255, 1, 1)
DIM GLOBAL MAP(7, 7)

POKENUM = 1
TEAMNUM = 5
DIM GLOBAL POKENAME$(POKENUM)
DIM GLOBAL TEAMID(TEAMNUM)

'SETUP
PLX = 16 * 4
PLY = 16 * 4
PLTX = PLX
PLTY = PLY

CALL LOADCOLL
CMAPX = -1
CMAPY = -1

TEXTSPEED = 2

CALL LOADPOKE

TEAMID(0) = 1

RANDOMIZE TIMER
GAMEPAD 1

'MAIN LOOP
MAIN:
CLS
SPRITE OFF
DO
  'GET INPUT
  H = LEFT(0) - RIGHT(0)
  V =   UP(0) -  DOWN(0)
  
	'PLAYER UPDATE
  GOSUB PLMOVE
  GOSUB PLANIM
	
	'MOVE CAMERA
	CAMX = PLX - 64
	CAMY = PLY - 64
	
	CALL DRAWBG(CAMX, CAMY)
	
	'MOVE BACKGROUND
	SCROLL 1, CAMX AND 7, CAMY AND 7
	SCROLL 0, CAMX AND 7, CAMY AND 7
  
  'COPY PLAYER SPRITE TO CHARACTER MEMORY
  CALL ROM2CHAR(5, PLFRM, 64, 2, 2)
  
	PLSX = PLX - CAMX
	PLSY = PLY - CAMY - 4
	
  'DISPLAY PLAYER
  SPRITE 63, PLSX, PLSY, 64
  SPRITE 63 SIZE 1 FLIP PLFLX, 0
	
	IF INGRASS THEN
		SPRITE 62, PLSX 	 , PLSY + 12, 6
		SPRITE 61, PLSX + 8, PLSY + 12, 6
		SPRITE 62 PAL 1
		SPRITE 61 PAL 1 FLIP 1,0
	ELSE
		SPRITE OFF 61 TO 62
	END IF
  
  'RUN ONCE A FRAME
  WAIT VBL
LOOP
PLMOVE:
	'IF PLAYER IS AT THE TARGET TILE
	IF PLX = PLTX AND PLY = PLTY THEN
		'INTERACTION
		IF BUTTON TAP(0,1) THEN
			'GET FACING TILE
			FACEX = PLX
			FACEY = PLY
			IF PLDIR = 0 THEN ADD FACEY, 16 ELSE IF PLDIR = 1 THEN ADD FACEY, -16 ELSE IF PLDIR = 2 THEN ADD FACEX, -16 ELSE ADD FACEX, 16
			
			CALL GETTILE(FACEX, FACEY)
			'SIGN
			IF OUT = 2 THEN
				CALL TEXTBOX("IN THE BEGINNING, GOD CREATED THE HEAVENS AND THE EARTH. THE EARTH WAS FORMLESS AND VOID, DARKNESS COVERED THE FACE OF THE DEEP, AND THE SPIRIT OF GOD WAS HOVERING OVER THE SURFACE OF THE WATERS. THEN GOD SAID, 'LET THERE BE LIGHT', AND THERE WAS LIGHT. GOD SAW THAT THE LIGHT WAS GOOD. EVENING PASSED AND MORNING CAME. THIS WAS THE FIRST DAY.")
			END IF
		END IF
		
		'IF THE PLAYER IS MOVING
		IF H <> 0 OR V <> 0 THEN
			'SET NEW TARGET IN HORIZONTAL OR VERTICAL DIRECTION
			IF H THEN
			ADD PLTX, H * 16
			IF H < 0 THEN PLDIR = 2 ELSE PLDIR = 3
			ELSE
			ADD PLTY, V * 16
			IF V < 0 THEN PLDIR = 1 ELSE PLDIR = 0
			END IF
			MOVE = -1
			
			'RUNNING SHOES (KEEP SPEED AT [1, 2, 4, 8, OR 16] TO AVOID ISSUES)
			IF BUTTON(0,0) THEN PLSPEED = 2 ELSE PLSPEED = 1
			
			CALL GETTILE(PLTX, PLTY)
			'WALL
			IF OUT = 1 OR OUT = 2 OR OUT = 9 OR ((OUT >= 5 AND OUT <= 8) AND OUT <> PLDIR + 5) THEN
				PLTX = PLX
				PLTY = PLY
			END IF
		ELSE
			MOVE = 0
		END IF
	ELSE
		'SLIDE THE PLAYER TOWARDS THE TARGET POSITION
		ADD PLX, SGN(PLTX - PLX) * PLSPEED
		ADD PLY, SGN(PLTY - PLY) * PLSPEED
		
		'PLAYER ENTERED A NEW TILE
		IF PLX = PLTX AND PLY = PLTY THEN
			CALL GETTILE(PLX, PLY)
			
			IF OUT = 3 THEN CALL WARP(RND(15),RND(15))
			
			INGRASS = OUT = 4
			IF INGRASS AND RND < 0.1 THEN
				GOTO BATTLE
			END IF
		END IF
	END IF
RETURN
PLANIM:
  'PLAYER ANIMATION
  PLFRM = 2 * PLDIR
  PLFLX = 0
  IF PLDIR = 3 THEN
    ADD PLFRM, -2
		PLFLX = -1
  END IF
  IF MOVE THEN
    'ANIMATION TIME
    WTIME = TIMER * 0.1
		WMOD = WTIME AND 1
		WDIV = (WTIME \ 2) AND 1
	
    IF PLDIR = 0 THEN
			ADD PLFRM, 32 * WMOD
      PLFLX = WDIV
    ELSE IF PLDIR = 1 THEN
			ADD PLFRM, 32 * WMOD
      PLFLX = WDIV
    ELSE IF PLDIR = 2 THEN
      ADD PLFRM, 32 * WMOD
    ELSE
			ADD PLFRM, 32 * WMOD
    END IF
  END IF
RETURN

BATTLE:
TRACE "BATTLE"
'DISPLAY TRANSITION (SUB)
CALL TRANSITION

ENEMYID = 0
ENEMYLV = 0

'ENCOUNTER TABLE
CHX = INT(PLX / 256)
CHY = INT(PLY / 256)
TRACE STR$(CHX)+","+STR$(CHY)
IF CHX = -1 AND CHY = 0 THEN
	RESTORE ENCOUNTER_0
ELSE
	RESTORE ENCOUNTER_0
END IF
RANDOM = RND
CHANCE = 0
NEXT_ENCOUNTER:
READ ID
IF ID THEN
	READ LV, CH
	ADD CHANCE, CH
	IF RANDOM < CHANCE THEN
		ENEMYID = ID
		ENEMYLV = LV
	ELSE
		GOTO NEXT_ENCOUNTER
	END IF
END IF

'DISPLAY ENEMY INFO
TEXT 1, 1, POKENAME$(ENEMYID)
TEXT 1, 2, "LV: " + STR$( ENEMYLV )

'DISPLAY PLAYER INFO
TEXT 9, 8, POKENAME$(TEAMID(0))
TEXT 9, 9, "LV: " + STR$( RND(40) + 10 )

'PLAYER & ENEMY SPRITE
SPRITE 0, 8, 128 - 64, 128
SPRITE 1, 160 - 40, 8, 1
SPRITE 0 SIZE 3
SPRITE 1 SIZE 3

DO
	IF BUTTON TAP(0) THEN GOTO MAIN
	
	WAIT VBL
LOOP

SUB TRANSITION
	PAL 2
	PRIO 1
	FOR I = 0 TO 1000
		TEXT RND(20), RND(16), " "
		IF RND < 0.08 THEN WAIT VBL
	NEXT I
	
	CLS
	SPRITE OFF
	
	PRIO 0
	BG FILL 0, 0 TO 31, 31 CHAR 192
END SUB

'INPUT: 
'ROM INDEX
'SOURCE INDEX 
'DESTINATION INDEX
'CHARACTER WIDTH
'CHARACTER HEIGHT
'OUTPUT:
'MOVES A WIDTH X HEIGHT AREA OF CHARACTERS IN ROM STARTING WITH THE SOURCE CHARACTER TO THE DESTINATION CHARACTER
SUB ROM2CHAR(R,S,D,W,H)
	SRC = ROM(R) + 16 * S
	DST = $8000  + 16 * D
	FOR Y = 0 TO H - 1
	  COPY SRC, 16 * W TO DST
		
		ADD SRC, 256
		ADD DST, 256
	NEXT Y
END SUB

SUB TEXTBOX(T$)
	PAL 2
	BG FILL 0, 12 TO 19, 15 CHAR 192
	
	'DRAW BORDERS
	FLIP 0,0
	TEXT 0, 12, "[\\\\\\\\\\\\\\\\\\"
	TEXT 0, 13, "]"
	TEXT 0, 14, "]"
	FLIP 1,0
	TEXT 19,12, "["
	TEXT 19,13, "]"
	TEXT 19,14, "]"
	FLIP 0,1
	TEXT 0, 15, "[\\\\\\\\\\\\\\\\\\"
	FLIP 1,1
	TEXT 19,15, "["
	FLIP 0,0
	
	TEXTX = 1
	TEXTY = 13
	DELAY = -1
	
	I = 1
	REPEAT
		'NEXT WORD WON'T FIT ON CURRENT LINE
		IF (INSTR(T$," ",I) - I + TEXTX) > 19 THEN
			'GO TO NEXT LINE
			TEXTX = 1
			INC TEXTY
			
			'TEXTBOX FULL
			IF TEXTY = 15 THEN
				'DISPLAY ARROW
				FLIP 0,0
				TEXT 9,15,"_"
				FLIP 1,0
				TEXT 10,15,"_"
				FLIP 0,0
				
				'WAIT UNTIL BUTTON IS PRESSED
				REPEAT
				UNTIL BUTTON TAP(0)
				
				'RESET TEXTBOX
				TEXTY = 13
				BG FILL 1, 13 TO 18, 14 CHAR 192
				
				FLIP 0,1
				TEXT 9,15,"\\"
				FLIP 0,0
				
				DELAY = -1
			END IF
		END IF
		
		'DISPLAY THE NEXT CHARACTER
		CC$ = MID$(T$,I,1)
		IF CC$ <> " " THEN TEXT TEXTX,TEXTY,CC$
		INC TEXTX
		
		'SELECT NEXT CHARACTER AND ADD A DELAY UNLESS IT IS DISABLED BY PRESSING THE SPEED-UP BUTTON
		INC I
		IF DELAY THEN
			FOR W = 1 TO (10 - TEXTSPEED)
				WAIT VBL
				IF BUTTON TAP(0) THEN DELAY = 0
			NEXT W
		END IF
	UNTIL I = LEN(T$) + 1
	
	'WAIT UNTIL BUTTON IS PRESSED
	REPEAT
	UNTIL BUTTON TAP(0)
END SUB

SUB WARP(TILEX,TILEY)
	PLX = TILEX * 16
	PLY = TILEY * 16
	PLTX = PLX
	PLTY = PLY
END SUB

SUB GETTILE(PX,PY)
	'LOAD CURRENT BACKGROUND
	CALL LOADBG(INT(PX / 256), INT(PY / 256))
		
	PLCX = (PX \ 16) AND 15
	PLCY = (PY \ 16) AND 15
	OUT = COLLISION(MAP(PLCX \ 2, PLCY \ 2), PLCX MOD 2, PLCY MOD 2)
END SUB

'TAKE COORDINATE, DRAW VISIBLE AREA TO SCREEN
SUB DRAWBG(X,Y)
  'STORE CURRENT CELL COORDINATES
	STARTX = -(INT(X / 8) AND 31)
	STARTY = -(INT(Y / 8) AND 31)
	
	CELLX = STARTX
	CELLY = STARTY
	
	BG 1
  'FOR EACH MAP ON SCREEN
	FOR MAPY = INT(Y / 256) TO INT((Y + 127) / 256)
	  FOR MAPX = INT(X / 256) TO INT((X + 159) / 256)
			CALL LOADBG(MAPX, MAPY)
			
			CALL DRAWMAP(CELLX,CELLY)
			
			ADD CELLX, 32
		NEXT MAPX
		CELLX = STARTX
		ADD CELLY, 32
	NEXT MAPY
END SUB
SUB DRAWMAP(CX,CY)
	MINX = MAX(0, INT(-CX / 4))
	MAXX = MIN(INT((-CX + 20) / 4), 7)
	MINY = MAX(0, INT(-CY / 4))
	MAXY = MIN(INT((-CY + 16) / 4), 7)
	
	FOR Y = MINY TO MAXY
		FOR X = MINX TO MAXX
		  XX = CX + X * 4
			YY = CY + Y * 4
			
			SRC = MAP(X, Y)
			SRCX = (SRC MOD 16) * 4
			SRCY = (SRC  \  16) * 4
			
			BG COPY SRCX, SRCY, 4, 4 TO XX, YY
		NEXT X
	NEXT Y
END SUB

SUB LOADBG(MX,MY)
	IF MX = CMAPX AND MY = CMAPY THEN EXIT SUB
	CMAPX = MX
	CMAPY = MY
	
	IF MX = 0 AND MY = 0 THEN
		RESTORE NEW_BARK
	ELSE IF MX = 0 AND MY = -1 THEN
		RESTORE NEW_BARK_TOP
	ELSE IF MX = -1 AND MY = 0 THEN
		RESTORE ROUTE29_0
	ELSE IF MX = -2 AND MY = 0 THEN
		RESTORE ROUTE29_1
	ELSE IF (MX = -1 AND MY = -1) OR ((MX = -2 OR MX = -1) AND MY = 1) OR (MX = 0 AND MY = 1) THEN
		RESTORE TREES_BG
	ELSE
		RESTORE EMPTY_BG
	END IF
	
  FOR Y = 0 TO 7
	  FOR X = 0 TO 7
		  READ MAP(X, Y)
		NEXT X
	NEXT Y
END SUB
